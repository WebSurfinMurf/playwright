{
  "name": "playwright-service",
  "version": "1.0.0",
  "description": "Playwright browser automation service with web interface and API",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "report": "playwright show-report",
    "install:browsers": "playwright install",
    "health": "curl -f http://localhost:3000/health"
  },
  "dependencies": {
    "@playwright/test": "^1.40.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.1",
    "axios": "^1.6.0",
    "redis": "^4.6.10",
    "ws": "^8.14.2",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "playwright",
    "browser-automation",
    "testing",
    "web-scraping",
    "api"
  ],
  "author": "AI Servicers Infrastructure",
  "license": "MIT"
}